{
  "name": "object-storage",
  "version": "0.0.1",
  "description": "A NestJS standalone app, that do actually nothing, but expose different provider to interact with various object storage services like AWS S3, MinIO, and Alibaba Cloud OSS.",
  "author": "Yoann Gendrey",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/client-sts": "^3.896.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@smithy/types": "^4.5.0",
    "ali-oss": "^6.23.0",
    "luxon": "^3.7.2",
    "minio": "^8.0.6",
    "nestjs-pino": "^4.4.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/core": "^1.13.20",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.10.7",
    "@vitest/coverage-v8": "^3.2.4",
    "globals": "^16.0.0",
    "source-map-support": "^0.5.21",
    "typescript": "^5.7.3",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4"
  }
}
